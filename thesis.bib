issn = {1520-4995},
journal = {Biochemistry},
keywords = {Amino Acid Sequence,Antiviral Agents,Antiviral Agents: chemistry,Antiviral Agents: metabolism,Antiviral Agents: pharmacology,Aptamers, Nucleotide,Aptamers, Nucleotide: chemistry,Aptamers, Nucleotide: metabolism,Aptamers, Nucleotide: pharmacology,Binding, Competitive,Conserved Sequence,Ebolavirus,Ebolavirus: drug effects,Ebolavirus: metabolism,Hemorrhagic Fever, Ebola,Hemorrhagic Fever, Ebola: drug therapy,Hemorrhagic Fever, Ebola: metabolism,Kinetics,Molecular Targeted Therapy,Mutant Proteins,Mutant Proteins: antagonists \& inhibitors,Mutant Proteins: chemistry,Mutant Proteins: metabolism,Nucleic Acid Conformation,Nucleoproteins,Nucleoproteins: metabolism,Protein Conformation,Protein Interaction Domains and Motifs,RNA,RNA, Double-Stranded,RNA, Double-Stranded: metabolism,RNA: chemistry,RNA: metabolism,RNA: pharmacology,Recombinant Proteins,Recombinant Proteins: chemistry,Recombinant Proteins: metabolism,SELEX Aptamer Technique,Species Specificity,Viral Regulatory and Accessory Proteins,Viral Regulatory and Accessory Proteins: antagonis,Viral Regulatory and Accessory Proteins: chemistry,Viral Regulatory and Accessory Proteins: genetics,Viral Regulatory and Accessory Proteins: metabolis},
