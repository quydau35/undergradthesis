file = {:home/quyngan/Documents/Ebola/Journal/Hub\_JCTC2010.pdf:pdf},
isbn = {1549-9618},
issn = {15499618},
journal = {Journal of Chemical Theory and Computation},
number = {12},
pages = {3713--3720},
