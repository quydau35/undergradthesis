journal = {Journal of virology},
keywords = {Animals,Cell Line,Cercopithecus aethiops,Ebolavirus,Ebolavirus: chemistry,STAT1 Transcription Factor,STAT1 Transcription Factor: antagonists \& inhibito,STAT1 Transcription Factor: metabolism,Signal Transduction,Vero Cells,Viral Proteins,Viral Proteins: pharmacology,alpha Karyopherins,alpha Karyopherins: drug effects,alpha Karyopherins: metabolism},
month = jun,
number = {11},
pages = {5156--67},
pmid = {16698996},
title = {{Ebola virus VP24 binds karyopherin alpha1 and blocks STAT1 nuclear accumulation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1472181\&tool=pmcentrez\&rendertype=abstract},
volume = {80},
year = {2006}
}
@article{Duan2003,
abstract = {Molecular mechanics models have been applied extensively to study the dynamics of proteins and nucleic acids. Here we report the development of a third-generation point-charge all-atom force field for proteins. Following the earlier approach of Cornell et al., the charge set was obtained by fitting to the electrostatic potentials of dipeptides calculated using B3LYP/cc-pVTZ//HF/6-31G** quantum mechanical methods. The main-chain torsion parameters were obtained by fitting to the energy profiles of Ace-Ala-Nme and Ace-Gly-Nme di-peptides calculated using MP2/cc-pVTZ//HF/6-31G** quantum mechanical methods. All other parameters were taken from the existing AMBER data base. The major departure from previous force fields is that all quantum mechanical calculations were done in the condensed phase with continuum solvent models and an effective dielectric constant of epsilon = 4. We anticipate that this force field parameter set will address certain critical short comings of previous force fields in condensed-phase simulations of proteins. Initial tests on peptides demonstrated a high-degree of similarity between the calculated and the statistically measured Ramanchandran maps for both Ace-Gly-Nme and Ace-Ala-Nme di-peptides. Some highlights of our results include (1) well-preserved balance between the extended and helical region distributions, and (2) favorable type-II poly-proline helical region in agreement with recent experiments. Backward compatibility between the new and Cornell et al. charge sets, as judged by overall agreement between dipole moments, allows a smooth transition to the new force field in the area of ligand-binding calculations. Test simulations on a large set of proteins are also discussed.},
author = {Duan, Yong and Wu, Chun and Chowdhury, Shibasish and Lee, Mathew C. and Xiong, Guoming and Zhang, Wei and Yang, Rong and Cieplak, Piotr and Luo, Ray and Lee, Taisung and Caldwell, James and Wang, Junmei and Kollman, Peter},
doi = {10.1002/jcc.10349},
file = {:home/quyngan/Documents/Ebola/Journal/duan2003.pdf:pdf},
isbn = {0192-8651},
issn = {01928651},
journal = {Journal of Computational Chemistry},
keywords = {Algorithms,Amino Acids,Amino Acids: chemistry,Chemistry,Computer Simulation,Databases,Dipeptides,Dipeptides: chemistry,Factual,Hydrogen Bonding,Mathematical Computing,Models,Molecular mechanics simulations,Peptides,Peptides: chemistry,Physical,Physicochemical Phenomena,Point-charge force field,Protein Conformation,Protein Structure,Proteins,Proteins: chemistry,Quantum Theory,Quantum mechanical calculations,Secondary,Solvents,Solvents: chemistry,Static Electricity,Theoretical,Thermodynamics,Water,Water: chemistry},
month = dec,
number = {16},
pages = {1999--2012},
pmid = {14531054},
title = {{A Point-Charge Force Field for Molecular Mechanics Simulations of Proteins Based on Condensed-Phase Quantum Mechanical Calculations}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14531054},
volume = {24},
year = {2003}
}
@article{Ponder2003,
abstract = {The chapter focuses on a general description of the force fields that are most commonly used at present, and it gives an indication of the directions of current research that may yield better functions in the near future. After a brief survey of current models, mostly generated during the 1990s, the focus of the chapter is on the general directions the field is taking in developing new models. The most commonly used protein force fields incorporate a relatively simple potential energy function: The emphasis is on the use of continuum methods to model the electrostatic effects of hydration and the introduction of polarizability to model the electronic response to changes in the environment. Some of the history and performance of widely used protein force fields based on an equation on simplest potential energy function or closely related equations are reviewed. The chapter outlines some promising developments that go beyond this, primarily by altering the way electrostatic interactions are treated. The use of atomic multipoles and off-center charge distributions, as well as attempts to incorporate electronic polarizability, are also discussed in the chapter.},
author = {Ponder, Jay W. and Case, David a.},
doi = {10.1016/S0065-3233(03)66002-X},
file = {:home/quyngan/Documents/Ebola/Journal/[doi 10.1016\%2FS0065-3233\%2803\%2966002-X] Ponder, Jay W. -- [Advances in Protein Chemistry] Protein Simulations Volume 66 Force Fields for Protein Simulations.pdf:pdf},
isbn = {9780120342662},
