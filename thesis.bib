keywords = {Animals,Cell Line,Cell Nucleus,Cell Nucleus: metabolism,Cercopithecus aethiops,DNA-Binding Proteins,DNA-Binding Proteins: metabolism,Ebolavirus,Ebolavirus: metabolism,Ebolavirus: pathogenicity,Humans,Interferon Regulatory Factor-3,Interferon-alpha,Interferon-alpha: metabolism,Interferon-beta,Interferon-beta: metabolism,Nucleoproteins,Nucleoproteins: physiology,Phosphorylation,Sendai virus,Sendai virus: pathogenicity,Transcription Factors,Transcription Factors: metabolism,Transcriptional Activation,Transfection,Vero Cells,Viral Core Proteins,Viral Core Proteins: physiology},
month = jul,
